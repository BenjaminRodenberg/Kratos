int main(int argc, char** argv)
{
    const bool print_results = (argc > 1 && strcmp(argv[1], "--print-results")==0);

    const ElementData data;

    ShapeFunGrad DN_DX;
    ShapeFun N;

    const double xi = -sqrt(3)/3;
    const double eta = sqrt(3)/3;
    ShapeFunctions<dim, nnodes>(N, DN_DX, xi, eta);

    TestResult result = TestResult::SUCCESS;
    result += test_substitute_rho_proj_2D(N, DN_DX, data, print_results);
    result += test_substitute_mom_proj_2D(N, DN_DX, data, print_results);
    result += test_substitute_tot_ener_proj_2D(N, DN_DX, data, print_results);
    result += test_substitute_lhs_2D_OSS(N, DN_DX, data, print_results);
    result += test_substitute_rhs_2D_OSS(N, DN_DX, data, print_results);
    result += test_substitute_lhs_2D_ASGS(N, DN_DX, data, print_results);
    result += test_substitute_rhs_2D_ASGS(N, DN_DX, data, print_results);
    
    return static_cast<int>(result);
}